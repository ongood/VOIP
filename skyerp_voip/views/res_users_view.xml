<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>

    <record id="voip_view_users_form" model="ir.ui.view">
        <field name="name">voip.res.users.form.view</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form" />
        <field name="arch" type="xml">
            <notebook position="inside" >
                <page string="VOIP" >
                    <group>
                        <field name="take_call" />
                        <field name="phone" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>
    
</data>
</odoo>